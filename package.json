{
    "name": "maestro-monorepo",
    "private": true,
    "workspaces": [
      "apps/*",
      "packages/*"
    ],
    "scripts": {
      "dev:web": "pnpm --filter web-internal dev",
      "build:web": "pnpm --filter web-internal build",
      "dev:api": "cd packages/api && uvicorn app.main:app --reload"
    },
    "devDependencies": {
      "typescript": "^5.8.2"
    }
  }